<ejb-jar-bnd
        xmlns="http://websphere.ibm.com/xml/ns/javaee"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://websphere.ibm.com/xml/ns/javaee http://websphere.ibm.com/xml/ns/javaee/ibm-ejb-jar-bnd_1_1.xsd"
        version="1.1">

    <session name="JmsSendAndForgetService" simple-binding-name="ejb/${ear.build.name}/session/JmsSendAndForgetService">
        <resource-ref name="jms/someManagedCF"
                      binding-name="${JmsSend.Resources.ConnectionFactory.BindingName}"></resource-ref>
        <resource-ref name="jms/someManagedDestination"
                      binding-name="${JmsSend.Resources.Destination.BindingName}"></resource-ref>
    </session>
    <session name="JmsSendAndWaitService" simple-binding-name="ejb/${ear.build.name}/session/JmsSendAndWaitService">
        <resource-ref name="jms/someManagedCF"
                      binding-name="${JmsSend.Resources.ConnectionFactory.BindingName}"></resource-ref>
        <resource-ref name="jms/someManagedDestination"
                      binding-name="${JmsSend.Resources.Destination.BindingName}"></resource-ref>
    </session>

    <session name="JmsSendAndForgetCachedConnectionTestService" simple-binding-name="ejb/${ear.build.name}/session/JmsSendAndForgetCachedConnectionTestService">
        <resource-ref name="jms/someManagedCF"
                      binding-name="${JmsSend.Resources.ConnectionFactory.BindingName}"></resource-ref>
        <resource-ref name="jms/someManagedDestination"
                      binding-name="${JmsSend.Resources.Destination.BindingName}"></resource-ref>
    </session>
    <session name="JmsSendAndForgetNonJCATestService"
             simple-binding-name="ejb/${ear.build.name}/session/JmsSendAndForgetNonJCATestService">
    </session>
</ejb-jar-bnd>