<ejb-jar
        xmlns="http://java.sun.com/xml/ns/javaee"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
                            http://java.sun.com/xml/ns/javaee/ejb-jar_3_1.xsd"
        version="3.1">
    <description>Partial deployment descriptors</description>
    <display-name>Partial deployment descriptors</display-name>
    <enterprise-beans>
        <session>
            <ejb-name>CounterService</ejb-name>
            <business-local>com.softwareaggov.messaging.libs.utils.Counter</business-local>
            <ejb-class>com.softwareaggov.messaging.libs.utils.impl.CounterImpl</ejb-class>
            <session-type>Singleton</session-type>
            <concurrency-management-type>Bean</concurrency-management-type>
        </session>
        <session>
            <ejb-name>JmsSendAndForgetService</ejb-name>
            <ejb-class>com.softwareaggov.messaging.simplejmssendoneway.ejb.publish.JmsSendAndForgetBean
            </ejb-class>
            <env-entry>
                <env-entry-name>jmsSendEnabled</env-entry-name>
                <env-entry-type>java.lang.Boolean</env-entry-type>
                <env-entry-value>${JmsSend.JmsSendAndForgetService.Env.jmsSendEnabled}</env-entry-value>
            </env-entry>
            <env-entry>
                <env-entry-name>jmsSessionTransacted</env-entry-name>
                <env-entry-type>java.lang.Boolean</env-entry-type>
                <env-entry-value>${JmsSend.JmsSendAndForgetService.Env.jmsSessionTransacted}</env-entry-value>
            </env-entry>
            <env-entry>
                <env-entry-name>jmsSessionAcknowledgeMode</env-entry-name>
                <env-entry-type>java.lang.Integer</env-entry-type>
                <env-entry-value>${JmsSend.JmsSendAndForgetService.Env.jmsSessionAcknowledgeMode}</env-entry-value>
            </env-entry>
            <env-entry>
                <env-entry-name>jmsDeliveryMode</env-entry-name>
                <env-entry-type>java.lang.Integer</env-entry-type>
                <env-entry-value>${JmsSend.JmsSendAndForgetService.Env.jmsDeliveryMode}</env-entry-value>
            </env-entry>
            <env-entry>
                <env-entry-name>jmsPriority</env-entry-name>
                <env-entry-type>java.lang.Integer</env-entry-type>
                <env-entry-value>${JmsSend.JmsSendAndForgetService.Env.jmsPriority}</env-entry-value>
            </env-entry>
            <resource-ref>
                <description>The JCA Managed Connection Factory for Message Publish</description>
                <res-ref-name>jms/someManagedCF</res-ref-name>
                <res-type>javax.jms.ConnectionFactory</res-type>
            </resource-ref>
            <resource-ref>
                <description>The JCA Managed Admin Object for Request Message Destination</description>
                <res-ref-name>jms/someManagedDestination</res-ref-name>
                <res-type>javax.jms.Destination</res-type>
            </resource-ref>
            <resource-ref>
                <description>The JCA Managed Admin Object for ReplyTo Message Destination</description>
                <res-ref-name>jms/someManagedReplyToDestination</res-ref-name>
                <res-type>javax.jms.Destination</res-type>
            </resource-ref>
