<?xml version="1.0" encoding="UTF-8"?>
<web-bnd
        xmlns="http://websphere.ibm.com/xml/ns/javaee"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://websphere.ibm.com/xml/ns/javaee http://websphere.ibm.com/xml/ns/javaee/ibm-web-bnd_1_0.xsd"
        version="1.0">

    <virtual-host name="default_host" />

    <resource-ref name="jms/someManagedCF" binding-name="SAGUMRAConnectionFactory">
    </resource-ref>
    <resource-ref name="jms/someManagedQCF" binding-name="SAGUMRAQueueConnectionFactory">
    </resource-ref>
    <resource-ref name="jms/someManagedTCF" binding-name="SAGUMRATopicConnectionFactory">
    </resource-ref>
    <resource-ref name="jms/someManagedSimpleQueue" binding-name="SAGUMSimpleQueue">
    </resource-ref>
    <resource-ref name="jms/someManagedRequestQueue" binding-name="SAGUMRequestQueue">
    </resource-ref>
    <resource-ref name="jms/someManagedResponseQueue" binding-name="SAGUMResponseQueue">
    </resource-ref>
</web-bnd>