<ejb-jar-bnd
        xmlns="http://websphere.ibm.com/xml/ns/javaee"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://websphere.ibm.com/xml/ns/javaee http://websphere.ibm.com/xml/ns/javaee/ibm-ejb-jar-bnd_1_1.xsd"
        version="1.1">

    <session name="JmsManagedSimplePublisherBean">
        <resource-ref name="jms/someManagedCF" binding-name="AppSimpleTestQueueConnectionFactory"></resource-ref>
        <resource-ref name="jms/someManagedSimpleDestination" binding-name="AppSimpleQueue"></resource-ref>
    </session>
    <session name="JmsManagedSimpleTopicPublisherBean">
        <resource-ref name="jms/someManagedCF" binding-name="AppSimpleTestTopicConnectionFactory"></resource-ref>
        <resource-ref name="jms/someManagedSimpleDestination" binding-name="AppSimpleTopic"></resource-ref>
    </session>
    <session name="JmsManagedSimpleCachedPublisherBean">
        <resource-ref name="jms/someManagedCF" binding-name="AppSimpleTestQueueConnectionFactory"></resource-ref>
        <resource-ref name="jms/someManagedSimpleDestination" binding-name="AppSimpleQueue"></resource-ref>
    </session>
    <session name="JmsManagedRequestReplyPublisherBean">
        <resource-ref name="jms/someManagedCF" binding-name="AppReqReplyQueueConnectionFactory"></resource-ref>
        <resource-ref name="jms/someManagedRequestDestination" binding-name="AppRequestQueue"></resource-ref>
    </session>
    <session name="JmsManagedRequestReplyCachedPublisherBean">
        <resource-ref name="jms/someManagedCF" binding-name="AppReqReplyQueueConnectionFactory"></resource-ref>
        <resource-ref name="jms/someManagedRequestDestination" binding-name="AppRequestQueue"></resource-ref>
    </session>

    <message-driven name="SimpleQueueConsumerBean">
        <jca-adapter activation-spec-binding-name="AppSimpleQueueConsumer" />
    </message-driven>
    <message-driven name="SimpleTopicConsumerBean">
        <jca-adapter activation-spec-binding-name="AppSimpleTopicConsumer" />
    </message-driven>
    <message-driven name="ReplyQueueConsumerBean">
        <jca-adapter activation-spec-binding-name="AppReplyConsumer" />
    </message-driven>
</ejb-jar-bnd>