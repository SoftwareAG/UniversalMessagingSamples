<?xml version="1.0" encoding="UTF-8"?>
<definitions name="RequestReplyService"
             targetNamespace="http://com.softwareaggov.messaging/j2ee-jms-examples/requestreply"
             xmlns="http://schemas.xmlsoap.org/wsdl/"
             xmlns:soapjms="http://www.w3.org/2010/soapjms/"
             xmlns:tns="http://com.softwareaggov.messaging/j2ee-jms-examples/requestreply"
             xmlns:xsd="http://www.w3.org/2001/XMLSchema"
             xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
             xmlns:wsam="http://www.w3.org/2007/05/addressing/metadata"
             xmlns:jms="http://schemas.xmlsoap.org/wsdl/jms/">
    <types>
        <xsd:schema>
            <xsd:import namespace="http://com.softwareaggov.messaging/j2ee-jms-examples/requestreply" schemaLocation="RequestReplyService_schema1.xsd"/>
        </xsd:schema>
    </types>
    <message name="performMultiplicationFromStringsResponse">
        <part name="parameters" element="tns:performMultiplicationFromStringsResponse">
        </part>
    </message>
    <message name="getRandomNumber">
        <part name="parameters" element="tns:getRandomNumber">
        </part>
    </message>
    <message name="performMultiplicationFromStrings">
        <part name="parameters" element="tns:performMultiplicationFromStrings">
        </part>
    </message>
    <message name="getRandomNumberResponse">
        <part name="parameters" element="tns:getRandomNumberResponse">
        </part>
    </message>
    <portType name="RequestReplyServicePortType">
        <operation name="performMultiplicationFromStrings">
            <input message="tns:performMultiplicationFromStrings" wsam:Action="http://com.softwareaggov.messaging/j2ee-jms-examples/requestreply/RequestReplyServicePortType/performMultiplicationFromStringsRequest">
            </input>
            <output message="tns:performMultiplicationFromStringsResponse" wsam:Action="http://com.softwareaggov.messaging/j2ee-jms-examples/requestreply/RequestReplyServicePortType/performMultiplicationFromStringsResponse">
            </output>
        </operation>
        <operation name="getRandomNumber">
            <input message="tns:getRandomNumber" wsam:Action="http://com.softwareaggov.messaging/j2ee-jms-examples/requestreply/RequestReplyServicePortType/getRandomNumberRequest">
            </input>
            <output message="tns:getRandomNumberResponse" wsam:Action="http://com.softwareaggov.messaging/j2ee-jms-examples/requestreply/RequestReplyServicePortType/getRandomNumberResponse">
            </output>
        </operation>
    </portType>
    <binding name="RequestReplyServicePortBinding" type="tns:RequestReplyServicePortType">
        <!--<soap:binding style="document" transport="http://www.w3.org/2010/soapjms/"/>-->
        <!--<soapjms:deliveryMode>2</soapjms:deliveryMode> &lt;!&ndash; persistent=2 &ndash;&gt;-->
        <!--<soapjms:priority>4</soapjms:priority>-->
        <!--<soapjms:destinationType>javax.jms.Queue</soapjms:destinationType>-->
        <operation name="performMultiplicationFromStrings">
            <soap:operation soapAction=""/>
            <input>
                <soap:body use="literal"/>
            </input>
            <output>
                <soap:body use="literal"/>
            </output>
        </operation>
        <operation name="getRandomNumber">
            <soap:operation soapAction=""/>
            <input>
                <soap:body use="literal"/>
            </input>
            <output>
                <soap:body use="literal"/>
            </output>
        </operation>
    </binding>
    <service name="RequestReplyService">

        <port name="RequestReplyServicePort" binding="tns:RequestReplyServicePortBinding">
            <!--<soapjms:deliveryMode>2</soapjms:deliveryMode> &lt;!&ndash; persistent=2 &ndash;&gt;-->
            <!--<soapjms:priority>4</soapjms:priority>-->
            <!--<soapjms:destinationType>javax.jms.Queue</soapjms:destinationType>-->
            <!--<soapjms:jndiConnectionFactoryName>AppReqReplyQueueConnectionFactory</soapjms:jndiConnectionFactoryName>-->

            <!-- With RA - Using Temporary queues for the reply -->
            <soap:address location="jms:jndi:AppSoapJmsRequestQueue?jndiConnectionFactoryName=AppReqReplyQueueConnectionFactory&amp;targetService=RequestReplyServicePortType"/>

            <!-- With RA - Using Permanent queue for the reply (using a RA reference for the replyTo causes exception so far...) -->
            <!--<soap:address location="jms:jndi:AppSoapJmsRequestQueue?jndiConnectionFactoryName=AppReqReplyQueueConnectionFactory&amp;targetService=RequestReplyServicePortType&amp;replyToName=AppReplyQueue"/>-->

            <!-- no using the RA -->
            <!--<soap:address location="jms:jndi:RequestQueue?jndiConnectionFactoryName=WebspherePublishQCF&amp;jndiInitialContextFactory=com.pcbsys.nirvana.nSpace.NirvanaContextFactory&amp;jndiURL=nsps://ip-172-30-1-187:9002&amp;targetService=RequestReplyJmsPort&amp;replyToName=ReplyQueue"/>-->
        </port>
    </service>
</definitions>
