                <env-entry-value>${jms.publish.priority}</env-entry-value>
            </env-entry>
        </session>
        <session>
            <ejb-name>JmsManagedRequestReplyCachedPublisherBean</ejb-name>
            <ejb-class>com.softwareaggov.messaging.service.publish.JmsManagedRequestReplyCachedPublisherBean</ejb-class>
            <resource-ref>
                <res-ref-name>jms/someManagedCF</res-ref-name>
                <res-type>javax.jms.QueueConnectionFactory</res-type>
                <jndi-name>java:/jms/AppReqReplyQueueConnectionFactory</jndi-name>
            </resource-ref>
            <resource-ref>
