<?xml version="1.1" encoding="UTF-8"?>
<jboss:ejb-jar xmlns:jboss="http://www.jboss.com/xml/ns/javaee"
               xmlns="http://java.sun.com/xml/ns/javaee"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xmlns:r="urn:resource-adapter-binding"
               xmlns:p="urn:ejb-pool:1.0"
               xsi:schemaLocation="http://www.jboss.com/xml/ns/javaee http://www.jboss.org/j2ee/schema/jboss-ejb3-2_0.xsd http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/ejb-jar_3_1.xsd"
               version="3.1"
               impl-version="2.0">

    <jboss:enterprise-beans>
        <session>
            <ejb-name>JmsManagedOneWayPublisherBean</ejb-name>
            <ejb-class>com.softwareaggov.messaging.simplejmssendoneway.ejb.publish.JmsManagedOneWayPublisherBean
            </ejb-class>
            <resource-ref>
                <res-ref-name>jms/someManagedCF</res-ref-name>
                <res-type>javax.jms.QueueConnectionFactory</res-type>
                <jndi-name>java:/jms/JmsQueueConnectionFactory</jndi-name>
            </resource-ref>
            <resource-ref>
                <res-ref-name>jms/someManagedDestination</res-ref-name>
                <res-type>com.sun.genericra.outbound.QueueProxy</res-type>
                <jndi-name>java:/jms/JmsQueueProxy</jndi-name>
            </resource-ref>
            <env-entry>
                <env-entry-name>jmsDeliveryMode</env-entry-name>
                <env-entry-type>java.lang.Integer</env-entry-type>
                <env-entry-value>${jms.publish.deliveryMode:-1}</env-entry-value>
            </env-entry>
            <env-entry>
                <env-entry-name>jmsPriority</env-entry-name>
                <env-entry-type>java.lang.Integer</env-entry-type>
                <env-entry-value>${jms.publish.priority:-1}</env-entry-value>
            </env-entry>
            <env-entry>
                <env-entry-name>jmsReplyDestinationName</env-entry-name>
                <env-entry-type>java.lang.String</env-entry-type>
                <env-entry-value>${jms.publish.replyDestinationName:}</env-entry-value>
            </env-entry>
            <env-entry>
                <env-entry-name>jmsReplyDestinationType</env-entry-name>
                <env-entry-type>java.lang.String</env-entry-type>
                <env-entry-value>${jms.publish.replyDestinationType:}</env-entry-value>
            </env-entry>
        </session>
        <session>
            <ejb-name>JmsManagedOneWayCachedPublisherBean</ejb-name>
            <ejb-class>com.softwareaggov.messaging.simplejmssendoneway.ejb.publish.JmsManagedOneWayCachedPublisherBean
            </ejb-class>
            <resource-ref>
                <res-ref-name>jms/someManagedCF</res-ref-name>
                <res-type>javax.jms.QueueConnectionFactory</res-type>
                <jndi-name>java:/jms/JmsQueueConnectionFactory</jndi-name>
            </resource-ref>
            <resource-ref>
                <res-ref-name>jms/someManagedDestination</res-ref-name>
                <res-type>com.sun.genericra.outbound.QueueProxy</res-type>
                <jndi-name>java:/jms/JmsQueueProxy</jndi-name>
            </resource-ref>
            <env-entry>
                <env-entry-name>jmsDeliveryMode</env-entry-name>
                <env-entry-type>java.lang.Integer</env-entry-type>
                <env-entry-value>${jms.publish.deliveryMode:-1}</env-entry-value>
            </env-entry>
            <env-entry>
                <env-entry-name>jmsPriority</env-entry-name>
                <env-entry-type>java.lang.Integer</env-entry-type>
                <env-entry-value>${jms.publish.priority:-1}</env-entry-value>
            </env-entry>
            <env-entry>
                <env-entry-name>jmsReplyDestinationName</env-entry-name>
                <env-entry-type>java.lang.String</env-entry-type>
                <env-entry-value>${jms.publish.replyDestinationName:}</env-entry-value>
            </env-entry>
            <env-entry>
                <env-entry-name>jmsReplyDestinationType</env-entry-name>
                <env-entry-type>java.lang.String</env-entry-type>
                <env-entry-value>${jms.publish.replyDestinationType:}</env-entry-value>
            </env-entry>
        </session>
        <session>
            <ejb-name>JmsManagedSendAndWaitBean</ejb-name>
            <ejb-class>com.softwareaggov.messaging.simplejmssendoneway.ejb.publish.JmsManagedSendAndWaitBean
            </ejb-class>
            <resource-ref>
                <res-ref-name>jms/someManagedCF</res-ref-name>
                <res-type>javax.jms.QueueConnectionFactory</res-type>
                <jndi-name>java:/jms/JmsQueueConnectionFactory</jndi-name>
            </resource-ref>
            <resource-ref>
                <res-ref-name>jms/someManagedDestination</res-ref-name>
                <res-type>com.sun.genericra.outbound.QueueProxy</res-type>
                <jndi-name>java:/jms/JmsQueueProxy</jndi-name>
            </resource-ref>
            <env-entry>
                <env-entry-name>jmsDeliveryMode</env-entry-name>
                <env-entry-type>java.lang.Integer</env-entry-type>
                <env-entry-value>${jms.publish.deliveryMode:-1}</env-entry-value>
            </env-entry>
            <env-entry>
                <env-entry-name>jmsPriority</env-entry-name>
                <env-entry-type>java.lang.Integer</env-entry-type>
                <env-entry-value>${jms.publish.priority:-1}</env-entry-value>
            </env-entry>
            <env-entry>
                <env-entry-name>jmsReplyDestinationName</env-entry-name>
                <env-entry-type>java.lang.String</env-entry-type>
                <env-entry-value>${jms.publish.replyDestinationName:JMSSamples/JmsResponseQueue}</env-entry-value>
            </env-entry>
            <env-entry>
                <env-entry-name>jmsReplyDestinationType</env-entry-name>
                <env-entry-type>java.lang.String</env-entry-type>
                <env-entry-value>${jms.publish.replyDestinationType:queue}</env-entry-value>
            </env-entry>
            <env-entry>
                <env-entry-name>jmsResponseWaitMillis</env-entry-name>
                <env-entry-type>java.lang.Long</env-entry-type>
                <env-entry-value>${jms.publish.jmsResponseWaitMillis:30000}</env-entry-value>
            </env-entry>
        </session>
        <session>
            <ejb-name>JMSNonJCAOneWayPublisherBean</ejb-name>
            <ejb-class>com.softwareaggov.messaging.simplejmssendoneway.ejb.publish.JMSNonJCAOneWayPublisherBean
            </ejb-class>
            <env-entry>
                <env-entry-name>jms.jndi.contextfactory</env-entry-name>
                <env-entry-type>java.lang.String</env-entry-type>
                <env-entry-value>com.pcbsys.nirvana.nSpace.NirvanaContextFactory</env-entry-value>
            </env-entry>
            <env-entry>
                <env-entry-name>jms.jndi.connection.url</env-entry-name>
                <env-entry-type>java.lang.String</env-entry-type>
                <env-entry-value>${jms.nonjca.jndi.connection.url:localhost:9000}</env-entry-value>
            </env-entry>
            <env-entry>
                <env-entry-name>jms.connectionfactory.name</env-entry-name>
                <env-entry-type>java.lang.String</env-entry-type>
                <env-entry-value>${jms.nonjca.connectionfactory.name:JBOSSPublishQCF}</env-entry-value>
            </env-entry>
            <env-entry>
                <env-entry-name>jms.default.destination.name</env-entry-name>
                <env-entry-type>java.lang.String</env-entry-type>
                <env-entry-value>${jms.nonjca.default.destination.name:JMSSamples/SimpleQueue}</env-entry-value>
            </env-entry>
            <env-entry>
                <env-entry-name>jmsReplyDestinationName</env-entry-name>
                <env-entry-type>java.lang.String</env-entry-type>
                <env-entry-value>${jms.publish.replyDestinationName:}</env-entry-value>
            </env-entry>
            <env-entry>
                <env-entry-name>jmsReplyDestinationType</env-entry-name>
                <env-entry-type>java.lang.String</env-entry-type>
                <env-entry-value>${jms.publish.replyDestinationType:}</env-entry-value>
            </env-entry>
        </session>
    </jboss:enterprise-beans>
    <assembly-descriptor>
        <r:resource-adapter-binding>
            <ejb-name>*</ejb-name>
            <r:resource-adapter-name>umra.rar</r:resource-adapter-name>
        </r:resource-adapter-binding>
        <p:pool>
            <ejb-name>*</ejb-name>
            <p:bean-instance-pool-ref>umra-strict-max-pool</p:bean-instance-pool-ref>
        </p:pool>
    </assembly-descriptor>
</jboss:ejb-jar>