<ejb-jar-bnd
        xmlns="http://websphere.ibm.com/xml/ns/javaee"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://websphere.ibm.com/xml/ns/javaee http://websphere.ibm.com/xml/ns/javaee/ibm-ejb-jar-bnd_1_1.xsd"
        version="1.1">

    <message-driven name="CalculationProcessingMDB">
        <jca-adapter activation-spec-binding-name="CalculationReqReplyConsumer"/>
        <resource-ref name="jms/someManagedCF" binding-name="CalculationReqReplyConnectionFactory">
        </resource-ref>
        <resource-ref name="jms/someManagedReplyTo" binding-name="CalculationReplyQueue">
        </resource-ref>
    </message-driven>
    <message-driven name="MockSleepProcessingMDB">
        <jca-adapter activation-spec-binding-name="MockSleepReqReplyConsumer"/>
        <resource-ref name="jms/someManagedCF" binding-name="MockSleepReqReplyConnectionFactory">
        </resource-ref>
        <resource-ref name="jms/someManagedReplyTo" binding-name="MockSleepReplyQueue">
        </resource-ref>
    </message-driven>
    <message-driven name="NoopProcessingMDB">
        <jca-adapter activation-spec-binding-name="NoopReqReplyConsumer"/>
        <resource-ref name="jms/someManagedCF" binding-name="NoopReqReplyConnectionFactory">
        </resource-ref>
        <resource-ref name="jms/someManagedReplyTo" binding-name="NoopReplyQueue">
        </resource-ref>
    </message-driven>
</ejb-jar-bnd>